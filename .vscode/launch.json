{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Animal example",
            "program": "${workspaceFolder}/testLibrary/Animal.mjs",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "debug Bible.mjs",
            "request": "launch",
            "runtimeArgs": [
                "main",
                "debug"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },

        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Bible.mjs"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch fetchDemo1.js",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/newTestLibrary/fetchDemo1.js"
        },
        {
            "name": "Launch books.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/books.html"
        },

        {
            "name": "Launch new and improved books2.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/books2.html"
        },
        {
            "name": "Launch fetch tests.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/testLibrary/fetch/fetchAPI1.html"
        }
    ]
}