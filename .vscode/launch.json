    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "debug Bible.mjs",
            "request": "launch",
            "runtimeArgs": [
                "main",
                "debug"
            ],
            "runtimeExecutable": "npm",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "name": "Debug index.html",
            "type": "chrome",
            "request": "debug",
            "file": "${workspaceFolder}/index.html"
        },
        {
            "command": "npm run testBible",
            "name": "testBible",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run main",
            "name": "main",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run testBook",
            "name": "testBook",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm run testChapter",
            "name": "testChapter",
            "request": "launch",
            "type": "node-terminal"
        },

        {
            "command": "npm run testAll",
            "name": "testAll",
            "request": "launch",
            "type": "node-terminal"
        },

        {
            "command": "npm run testStar",
            "name": "testStar",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "command": "npm start",
            "name": "Run npm start",
            "request": "launch",
            "type": "node-terminal"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Bible.mjs ?",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/Bible.mjs"
        },
        {
            "name": "Open books.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/books.html"
        },

        {
            "name": "Open books.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/books.html"
        },
        {
            "name": "Open book.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/book.html"
        },

        {
            "name": "Open new and improved books2.html",
            "type": "chrome",
            "request": "launch",
            "file": "${workspaceFolder}/books2.html"
        }
    ]
}